.status-waiting, .status-open {
  color: #babd3c;
}
.status-inprogress {
  color: #b67b0b;
}
.status-replay{
  color: #803b63;
}
.status-transferring,.status-paid, .status-answered {
  color: #293e80;
}
.status-failed,.status-cancelled {
  color: #802528;
}
.status-success {
  color: #3d8027;
}
.status-closed {
  color: black;
}